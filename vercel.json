{
  "builds": [
    { 
      "src": "server/server.py", 
      "use": "@vercel/python"
    },
    { 
      "src": "client/package.json", 
      "use": "@vercel/static-build"
    }
  ],
  "routes": [
    { "src": "/server/(.*)", "dest": "server/server.py" },
    { "src": "/client/(.*)", "dest": "client/page.tsx" }
  ]
}
